@phdthesis{Vazquez2017,
abstract = {En este trabajo se pretende delimitar el riesgo de incendios de la zona de interfaz urbano-forestal (ZIUF) del municipio de N{\'{a}}quera, en el Camp de Turia (Valencia). Para ello, se emplear{\'{a}}n tecnolog{\'{i}}as de informaci{\'{o}}n geogr{\'{a}}fica (TIG) y algunas de las herramientas que las constituyen. Para obtener los resultados deseados se usar{\'{a}} la informaci{\'{o}}n que proporciona, por un lado, el Sistema de Informaci{\'{o}}n sobre la Ocupaci{\'{o}}n del Suelo en Espa{\~{n}}a (SIOSE) y por otro, los datos que ofrece el LIDAR (light detection and ranging). Tambi{\'{e}}n se definir{\'{a}}n las escalas de trabajo m{\'{a}}s adecuadas para cada una de las fuentes, dependiendo del {\'{a}}mbito geogr{\'{a}}fico. El primer estudio se realizar{\'{a}} a nivel municipal. En base a esta informaci{\'{o}}n se estudiar{\'{a}}n seis zonas con mayor detalle. Finalmente, se realizar{\'{a}} una comparaci{\'{o}}n, que ponga en relieve la situaci{\'{o}}n en cada uno de las zonas analizadas y las virtudes e inconvenientes del uso de las dos fuentes anteriormente citadas.},
author = {V{\'{a}}zquez, Anna},
file = {:home/benizar/Documentos/Mendeley Desktop/2017 - V{\'{a}}zquez - Las TIG para la delimitaci{\'{o}}n de las ZIUF de N{\'{a}}quera (Camp de Turia).pdf:pdf},
keywords = {LIDAR,SIOSE,geographic information technologies (GIT),incendios forestales,interfaz urbano-forestal,lidar,siose,tecnolog{\'{i}}as de informaci{\'{o}}n geogr{\'{a}}fica,wildfires,wildland-urban interface (WUI)},
mendeley-tags = {LIDAR,SIOSE,geographic information technologies (GIT),wildfires,wildland-urban interface (WUI)},
pages = {71},
school = {Universidad de Alicante},
title = {{Las TIG para la delimitaci{\'{o}}n de las ZIUF de N{\'{a}}quera (Camp de Turia)}},
type = {Master},
year = {2017}
}
