editor = {{Universidad de Alicante}},
file = {:home/benizar/Documentos/Mendeley Desktop/2015{\_}Zaragoz{\'{i}} et al.{\_}Una plataforma web para aplicar t{\'{e}}cnicas de visualizaci{\'{o}}n de datos e investigar su uso en did{\'{a}}ctica de la Geog.pdf:pdf},
keywords = {aplicaci{\'{o}}n web,aprendizaje interactivo,constructivismo,datos locales,pir{\'{a}}mides de poblaci{\'{o}}n},
pages = {14},
title = {{Una plataforma web para aplicar t{\'{e}}cnicas de visualizaci{\'{o}}n de datos e investigar su uso en did{\'{a}}ctica de la Geograf{\'{i}}a}},
year = {2015}
}
